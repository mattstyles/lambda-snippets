".source.js, .source.ts, .source.jsx, .source.tsx, source.css, source.less":
  "Remark":
    prefix: "rem"
    body: """
    /**
     * ${1}
     */
    ${2}
    """
  "Large Remark":
    prefix: "remm"
    body: """
    /*-----------------------------------------------------------*\\\\
     *
     *  $1
     *
    \\\\*-----------------------------------------------------------*/
    $2
    """
